{
  "name"            :"Dark by Default",
  "version"         :"0.4",
  "manifest_version":2,
  "description"     :"Makes every page dark by default",
  "content_scripts" :[
    {
      "exclude_globs"  :[],
      "exclude_matches":[],
      "include_globs"  :["*"],
      "js"             :["dbd.user.js"],
      "matches"        :["http://*/*", "https://*/*"],
      "run_at"         :"document_start"
    }
  ]
}
